<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <attr name="font" format="string" />
    <attr name="color" format="color|reference" />
    
    <attr name="text" format="string" />
    <attr name="textColor" format="color|reference" />
    <attr name="textSize" format="dimension|reference" />
    
    <declare-styleable name="FontTextView" >
        <attr name="font" />
    </declare-styleable>
    
    <declare-styleable name="RiseView" >
        <attr name="rise_range" format="dimension|reference" />
        <attr name="fall_range" format="dimension|reference" />
    </declare-styleable>
    
    <declare-styleable name="AlarmClockBlock" >
        <attr name="font" />
        <attr name="color" />
    </declare-styleable>
    
    <declare-styleable name="SlideSwitcher">
        <attr name="textSize" />
        <attr name="textColor" />
    </declare-styleable>
    
    <declare-styleable name="DebossFontText">
        <attr name="font" />
        <attr name="text" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="textPaddingOffset" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="DialTimePicker">
        <attr name="color" />
    </declare-styleable>
    
</resources>
